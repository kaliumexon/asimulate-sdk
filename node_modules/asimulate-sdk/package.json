{
  "name": "asimulate-sdk",
  "version": "1.0.0",
  "description": "Advanced Physics Simulation Library - A comprehensive TypeScript SDK for physics simulation, recording, analysis, and visualization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean",
    "build:dts": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts --format cjs,esm --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "docs": "typedoc src/index.ts --out docs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "physics",
    "simulation",
    "engine",
    "3d",
    "2d",
    "collision",
    "dynamics",
    "rigid-body",
    "game-engine",
    "scientific",
    "analysis",
    "visualization",
    "typescript"
  ],
  "author": "ASIMULATE Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliumexon/asimulate-sdk"
  },
  "bugs": {
    "url": "https://github.com/kaliumexon/asimulate-sdk/issues"
  },
  "homepage": "https://asimulate.world/",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "typedoc": "^0.25.4"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
